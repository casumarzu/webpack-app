{
  "name": "webpack-app",
  "version": "1.0.0",
  "description": "webpack app",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register './client/**/*.test.js'",
    "clean": "rimraf dist",
    "start": "npm run dev",
    "dev": "NODE_ENV=dev node devServer",
    "dev:config": "NODE_ENV=dev nodemon --debug server",
    "prod": "npm run clean && NODE_ENV=prod webpack --progress --colors",
    "server": "node --harmony server",
    "deploy": "git push heroku master && heroku ps:scale web=1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/casumarzu/webpack-app.git"
  },
  "keywords": [
    "webpack",
    "app"
  ],
  "author": "Andrey Ivanov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/casumarzu/webpack-app/issues"
  },
  "homepage": "https://github.com/casumarzu/webpack-app",
  "devDependencies": {
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0"
  },
  "dependencies": {
    "fbjs": "^0.8.0-alpha.3",
    "material-ui": "^0.15.0-alpha.2"
  }
}
